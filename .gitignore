# Visual Studio 2015 user specific files
ShooterPrototype/.vs/
ProjectFPS/.vs/

# Compiled Object files
ShooterPrototype/*.slo
ShooterPrototype/*.lo
ShooterPrototype/*.o
ShooterPrototype/*.obj

ProjectFPS/*.slo
ProjectFPS/*.lo
ProjectFPS/*.o
ProjectFPS/*.obj

# Precompiled Headers
ShooterPrototype/*.gch
ShooterPrototype/*.pch

ProjectFPS/*.gch
ProjectFPS/*.pch

# Compiled Dynamic libraries
ShooterPrototype/*.so
ShooterPrototype/*.dylib
ShooterPrototype/*.dll

ProjectFPS/*.so
ProjectFPS/*.dylib
ProjectFPS/*.dll

# Fortran module files
ShooterPrototype/*.mod
ProjectFPS/*.mod

# Compiled Static libraries
ShooterPrototype/*.lai
ShooterPrototype/*.la
ShooterPrototype/*.a
ShooterPrototype/*.lib

ProjectFPS/*.lai
ProjectFPS/*.la
ProjectFPS/*.a
ProjectFPS/*.lib

# Executables
ShooterPrototype/*.exe
ShooterPrototype/*.out
ShooterPrototype/*.app
ShooterPrototype/*.ipa

ProjectFPS/*.exe
ProjectFPS/*.out
ProjectFPS/*.app
ProjectFPS/*.ipa

# These project files can be generated by the engine
ShooterPrototype/*.xcodeproj
ShooterPrototype/*.xcworkspace
ShooterPrototype/*.sln
ShooterPrototype/*.suo
ShooterPrototype/*.opensdf
ShooterPrototype/*.sdf
ShooterPrototype/*.VC.db
ShooterPrototype/*.VC.opendb

ProjectFPS/*.xcodeproj
ProjectFPS/*.xcworkspace
ProjectFPS/*.sln
ProjectFPS/*.suo
ProjectFPS/*.opensdf
ProjectFPS/*.sdf
ProjectFPS/*.VC.db
ProjectFPS/*.VC.opendb

# Precompiled Assets
SourceArt/**/*.png
SourceArt/**/*.tga

# Binary Files
ShooterPrototype/Binaries/*
ShooterPrototype/Plugins/*/Binaries/*

ProjectFPS/Binaries/*
ProjectFPS/Plugins/*/Binaries/*

# Builds
ShooterPrototype/Build/*
ProjectFPS/Build/*

# Whitelist PakBlacklist-<BuildConfiguration>.txt files
!ShooterPrototype/Build/*/
ShooterPrototype/Build/*/**
!ShooterPrototype/Build/*/PakBlacklist*.txt

!ProjectFPS/Build/*/
ProjectFPS/Build/*/**
!ProjectFPS/Build/*/PakBlacklist*.txt

# Don't ignore icon files in Build
!ShooterPrototype/Build/**/*.ico
!ProjectFPS/Build/**/*.ico

# Built data for maps
*_BuiltData.uasset

# Configuration files generated by the Editor
ShooterPrototype/Saved/*
ProjectFPS/Saved/*

# Compiled source files for the engine to use
ShooterPrototype/Intermediate/*
ShooterPrototype/Plugins/*/Intermediate/*

ProjectFPS/Intermediate/*
ProjectFPS/Plugins/*/Intermediate/*

# Cache files for the editor to use
ShooterPrototype/DerivedDataCache/*
ProjectFPS/DerivedDataCache/*

Package/
